{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "entry": ["src/routes/**/*.{ts,svelte}", "src/app.html"],
  "project": ["src/**/*.{ts,svelte}"],
  "ignore": ["src/lib/components/ui/**/*", "src/lib/components/shared/**/*"],
  "ignoreBinaries": ["tauri"],
  "ignoreExportsUsedInFile": {
    "interface": true,
    "type": true
  },
  "rules": {
    "exports": "off"
  }
}
